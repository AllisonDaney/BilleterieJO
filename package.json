{
  "name": "nuxt-app",
  "type": "module",
  "private": true,
  "packageManager": "pnpm@10.7.1",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "npx nuxthub preview",
    "deploy": "npx nuxthub deploy",
    "start": "pnpm build && pnpm preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "drizzle:generate": "drizzle-kit generate",
    "drizzle:migrate": "drizzle-kit migrate",
    "drizzle:push": "drizzle-kit push",
    "drizzle:up": "pnpm drizzle:generate && pnpm drizzle:migrate",
    "drizzle:seed": "tsx server/database/seed/index.ts",
    "test:drizzle:seed": "tsx server/database/seed/index.test.ts",
    "test:drizzle:push": "drizzle-kit push --config drizzle.config.test.ts",
    "test": "vitest"
  },
  "dependencies": {
    "@bitjson/qr-code": "^1.0.2",
    "@nuxt/fonts": "0.11.2",
    "@nuxt/image": "1.10.0",
    "@nuxt/ui": "3.1.0",
    "@nuxthub/core": "^0.8.23",
    "@pinia/nuxt": "0.11.0",
    "@stripe/stripe-js": "^7.3.0",
    "@vueuse/core": "^13.1.0",
    "@vueuse/nuxt": "^13.1.0",
    "axios": "^1.9.0",
    "bcryptjs": "^3.0.2",
    "cloudinary": "^2.6.1",
    "dotenv": "^16.5.0",
    "drizzle-orm": "^0.43.1",
    "drizzle-seed": "^0.3.1",
    "i18next": "^25.0.2",
    "jose": "^6.0.11",
    "nuxt": "^3.16.2",
    "ofetch": "^1.4.1",
    "pinia": "^3.0.2",
    "postgres": "^3.4.5",
    "qr-image": "^3.2.0",
    "stripe": "^18.1.0",
    "vue": "^3.5.13",
    "vue-qrcode-reader": "^5.7.2",
    "vue-router": "^4.5.0",
    "zod": "^3.24.3",
    "zod-i18n-map": "^2.27.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.12.0",
    "@nuxt/eslint": "^1.3.0",
    "@nuxt/test-utils": "^3.19.0",
    "@types/qr-image": "^3.2.9",
    "@types/validator": "^13.15.0",
    "@vitest/coverage-v8": "^3.1.4",
    "@vue/test-utils": "^2.4.6",
    "drizzle-kit": "^0.31.1",
    "eslint": "^9.23.0",
    "eslint-plugin-format": "^1.0.1",
    "happy-dom": "^17.4.7",
    "playwright-core": "^1.52.0",
    "tsx": "^4.19.4",
    "typescript": "^5.8.2",
    "vitest": "^3.1.4",
    "vue-tsc": "^2.2.8",
    "wrangler": "^4.6.0"
  }
}
