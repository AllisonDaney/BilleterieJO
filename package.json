{
  "name": "nuxt-app",
  "type": "module",
  "private": true,
  "packageManager": "pnpm@10.7.1",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "npx nuxthub preview",
    "deploy": "npx nuxthub deploy",
    "start": "pnpm build && pnpm preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "drizzle:generate": "drizzle-kit generate",
    "drizzle:migrate": "drizzle-kit migrate",
    "drizzle:push": "drizzle-kit push",
    "drizzle:up": "pnpm drizzle:generate && pnpm drizzle:migrate",
    "drizzle:seed": "tsx server/database/seed/index.ts"
  },
  "dependencies": {
    "@nuxt/fonts": "0.11.2",
    "@nuxt/image": "1.10.0",
    "@nuxt/ui": "3.1.0",
    "@nuxthub/core": "^0.8.23",
    "@pinia/nuxt": "0.11.0",
    "@vueuse/core": "^13.1.0",
    "@vueuse/nuxt": "^13.1.0",
    "bcryptjs": "^3.0.2",
    "dotenv": "^16.5.0",
    "drizzle-orm": "^0.43.1",
    "drizzle-seed": "^0.3.1",
    "i18next": "^25.0.2",
    "jose": "^6.0.11",
    "nuxt": "^3.16.2",
    "pinia": "^3.0.2",
    "postgres": "^3.4.5",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0",
    "zod": "^3.24.3",
    "zod-i18n-map": "^2.27.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.12.0",
    "@nuxt/eslint": "^1.3.0",
    "@types/validator": "^13.15.0",
    "drizzle-kit": "^0.31.1",
    "eslint": "^9.23.0",
    "eslint-plugin-format": "^1.0.1",
    "tsx": "^4.19.4",
    "typescript": "^5.8.2",
    "vue-tsc": "^2.2.8",
    "wrangler": "^4.6.0"
  }
}
